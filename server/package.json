{
  "name": "mev-backend",
  "version": "1.0.0",
  "description": "MEV交易监控系统后端",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "mock:push": "npx tsx scripts/auth-pusher.ts",
    "mock:push:ws": "npx tsx scripts/auth-pusher.ts ws",
    "mock:push:simple": "npx tsx scripts/mock-pusher.ts",
    "mock:push:simple:ws": "npx tsx scripts/mock-pusher.ts ws",
    "user:init": "npx tsx scripts/user-manager.ts init",
    "user:create": "npx tsx scripts/user-manager.ts create",
    "user:list": "npx tsx scripts/user-manager.ts list",
    "user:delete": "npx tsx scripts/user-manager.ts delete",
    "user:enable": "npx tsx scripts/user-manager.ts enable",
    "user:disable": "npx tsx scripts/user-manager.ts disable",
    "user:test": "npx tsx scripts/user-manager.ts test",
    "user:fix": "npx tsx scripts/user-manager.ts fix",
    "migrate:analyze": "npx tsx scripts/migrate-database.ts analyze",
    "migrate:run": "npx tsx scripts/migrate-database.ts migrate",
    "migrate:report": "npx tsx scripts/migrate-database.ts report",
    "debug:login": "npx tsx scripts/debug-login.ts",
    "test:api": "npx tsx scripts/test-api.ts"
  },
  "dependencies": {
    "fastify": "^4.24.3",
    "@fastify/cors": "^8.4.0",
    "@fastify/jwt": "^7.2.4",
    "@fastify/websocket": "^8.3.1",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^2.1.0",
    "@fastify/rate-limit": "^8.0.3",
    "@fastify/helmet": "^11.1.1",
    "@fastify/compress": "^6.4.0",
    "typeorm": "^0.3.17",
    "sqlite3": "^5.1.6",
    "bcryptjs": "^2.4.3",
    "zod": "^3.22.4",
    "dayjs": "^1.11.10",
    "socket.io": "^4.7.4",
    "node-cron": "^3.0.3",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "reflect-metadata": "^0.1.13",
    "@faker-js/faker": "^8.3.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/node": "^20.8.10",
    "@types/bcryptjs": "^2.4.6",
    "@types/node-cron": "^3.0.11",
    "@types/ws": "^8.5.10",
    "typescript": "^5.2.2",
    "tsx": "^4.1.4",
    "eslint": "^8.52.0",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.7"
  }
}